/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // aの結果
    Not(in=sel, out=c1);
    And(a=in, b=c1, out=a);
    // bの結果
    And(a=in, b=sel, out=b);
}
